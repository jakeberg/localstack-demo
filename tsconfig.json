{
  "extends": "@tsconfig/node14/tsconfig.json",
  "compilerOptions": {
    "outDir": "./dist",
    "declaration": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitAny": true,
    "preserveConstEnums": true,
    "useUnknownInCatchVariables": false
  },
  "exclude": ["dist", "scratchpad.ts"]
}
